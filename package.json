{
  "name": "effect-ts",
  "private": true,
  "scripts": {
    "build": "ultra -r -b yarn ets:build",
    "watch": "yarn ets:build -w",
    "clean": "ultra -r yarn ets:clean",
    "ets:build": "cd $INIT_CWD && ttsc -b tsconfig.json",
    "ets:watch": "cd $INIT_CWD && ttsc -p tsconfig.json --watch",
    "ets:clean": "cd $INIT_CWD && rimraf lib tsconfig.tsbuildinfo"
  },
  "packageManager": "yarn@3.0.2",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@types/node": "^15.12.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-codegen": "^0.15.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "husky": "6.0.0",
    "prettier": "^2.3.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "ttypescript": "^1.5.12",
    "typescript": "4.4.0-dev.20210625",
    "ultra-runner": "^3.10.5"
  }
}
